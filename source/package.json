  "name": "@3cx/callus",
  "version": "1.0.2",
  "description": "3CX Click2Call Integration",
  "main": "dist/callus.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --progress --profile --watch --content-base src/",
    "start:hot": "npm run start -- --inline --hot",
    "test": "karma start --browsers ChromeMediaHeadless",
    "myphone": "pbjs -o index.json GenericMessage.proto && pbjs -t protobuf/static-module --es6 -w commonjs --force-number --no-delimited --no-verify --no-create --no-convert -o src/generic/index.js GenericMessage.proto && pbts -o src/generic/index.d.ts src/generic/index.js && replace Partial\\. Partial src/generic/index.d.ts -s",
    "lint": "tslint src/**/*.ts",
    "zip": "node zip.js",
    "build": "webpack --mode production --profile --bail",
    "build:profile": "webpack --mode production --json",
    "build:ci": "npm run test && npm run build && npm run zip"
  },
  "publishConfig": {
    "registry": "http://debian.3cx.local:8081/repository/npm-hosted/"
  },
